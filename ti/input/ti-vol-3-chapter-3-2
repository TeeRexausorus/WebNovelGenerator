		function enableNightmode() {
			jQuery('html').addClass('nightmode');
			localStorage.setItem('nightmode', 'y');
		}

		function disableNightmode() {
			jQuery('html').removeClass('nightmode'); 
			localStorage.removeItem('nightmode');
