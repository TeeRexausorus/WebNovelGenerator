                        'hitType': 'event',
                        'eventCategory': 'AdBlocker',
                        'eventAction': 'Not Blocked',
                        'nonInteraction': 1
                    });

                    console.log('No Ad Blocker');
                }
            }, 2000);
        }

    
       detector();
})();
