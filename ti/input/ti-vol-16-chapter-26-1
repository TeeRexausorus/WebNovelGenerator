        var detector = function() {
            setTimeout(function() {

                if(!document.getElementsByClassName) return;
                var ads = document.getElementsByClassName('banner_ad'),
                    ad  = ads[ads.length - 1];

                if(!ad || ad.innerHTML.length == 0 || ad.clientHeight === 0) {
